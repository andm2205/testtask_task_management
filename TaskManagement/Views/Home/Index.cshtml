@model IEnumerable<AppTask>

<aside id="task_list_container">
    <input type="button"
        value="Создать новую задачу"
        onclick="return jQueryAjaxOpenForm('@Url.Action("CreateTask", "Home")');" />
    @await Html.PartialAsync("TaskList", Model)
</aside>
<article id="task_desc_container">
    @await Html.PartialAsync("TaskDesc", null)
</article>